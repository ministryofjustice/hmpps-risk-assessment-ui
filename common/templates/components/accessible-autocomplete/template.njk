<div class="govuk-form-group {% if error %}govuk-form-group--error{% endif %}">
  {%if label %}
  <fieldset class="govuk-fieldset">
    <legend class="govuk-fieldset__legend govuk-fieldset__legend--l">
        <h1 class="govuk-fieldset__heading">{{ label }}</h1>
    </legend>
  </fieldset>
  {% endif %}
  {% if error %}
    <span id="{{ id }}-error" class="govuk-error-message">
      <span class="govuk-visually-hidden">Error: </span>{{ error }}
    </span>
  {% endif %}
  <div class="govuk-form-group">
    <select id="{{ id }}" name="{{ id }}"class="govuk-body">
        <option value="">Please select from the list</option>
            {% for item in list %}
                <option value="{{item.value}}" {% if item.selected %}selected{% endif %}>{{item.text}}</option>
            {% endfor %}
    </select>
  </div>
  <script>
    window.accessibleAutocomplete.enhanceSelectElement({
      selectElement: document.querySelector('#{{ id }}'),
      showAllValues: false,
      autoselect: false,
      defaultValue: '',
    })
  </script>
</div>